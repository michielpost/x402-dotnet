<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>nullable</WarningsAsErrors>
		<Version>0.0.4</Version>
		<Authors>Michiel Post</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Description>x402 Payment Required Coinbase facilitator client for .net</Description>
		<PackageProjectUrl>https://github.com/michielpost/x402-dotnet</PackageProjectUrl>
		<RepositoryUrl>https://github.com/michielpost/x402-dotnet</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageIcon>x402-icon-blue.png</PackageIcon>
		<RepositoryType>GitHub</RepositoryType>
		<PackageTags>x402 crypto blockchain coinbase ai agents mcp</PackageTags>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<IncludeSymbols>True</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../../images/x402-icon-blue.png" Pack="true" PackagePath="\" />
		<None Include="../../README.md" Pack="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="BouncyCastle.NetCore" Version="2.2.1" />
		<PackageReference Include="Microsoft.Extensions.Http" Version="9.0.9" />
		<PackageReference Include="Microsoft.IdentityModel.Tokens" Version="8.14.0" />
		<PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="8.14.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\x402\x402.csproj" />
	</ItemGroup>

</Project>